<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane maxHeight="400.0" maxWidth="750.0" minHeight="400.0" minWidth="750.0" prefHeight="400.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="195.0" minWidth="10.0" prefWidth="195.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="135.0" minWidth="10.0" prefWidth="135.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="150.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="150.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="150.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="70.0" minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="250.0" minHeight="10.0" prefHeight="250.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
    <TableView fx:id="tableViewPredmeti" prefHeight="200.0" prefWidth="750.0" GridPane.columnSpan="6" GridPane.rowIndex="1" GridPane.rowSpan="2">
        <columns>
        <TableColumn fx:id="colPredmetId" prefWidth="75.0" text="ID" />
        <TableColumn fx:id="colPredmetOdjel" prefWidth="100" text="Odjeljenje" />
        <TableColumn fx:id="colPredmetGrupa" prefWidth="90" text="Grupa" />
        <TableColumn fx:id="colPredmetDuzina" prefWidth="90" text="Broj ucenika" />
        <TableColumn fx:id="colPredmetPredmet" prefWidth="315.0" text="Predmet" />
        <TableColumn fx:id="colPredmetBrojSati" prefWidth="75" text="Broj sati" />
        </columns>
    </TableView>
   <Button fx:id="dodajPr" mnemonicParsing="false" onAction="#dodajPredmet" prefHeight="10.0" prefWidth="110.0" text="Dodaj predmet" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.valignment="TOP">
      <GridPane.margin>
         <Insets top="5.0" />
      </GridPane.margin></Button>
   <Label fx:id="naslov" prefHeight="51.0" prefWidth="268.0" text="Label" wrapText="true" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
      <font>
         <Font size="25.0" />
      </font>
   </Label>
   <Button fx:id="izlazIzForme" mnemonicParsing="false" onAction="#izlaz" prefHeight="50.0" prefWidth="110.0" text="Izlaz" GridPane.columnIndex="5" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
   <Button fx:id="obrisiPr" mnemonicParsing="false" onAction="#obrisiPredmet" prefHeight="10.0" prefWidth="110.0" text="Obrisi predmet" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.valignment="TOP">
      <GridPane.margin>
         <Insets top="5.0" />
      </GridPane.margin></Button>
   <Label prefHeight="27.0" prefWidth="440.0" text="Dupli klik na predmet za dodavanje opisa i drugi stavki" GridPane.columnSpan="4" GridPane.rowIndex="3">
      <font>
         <Font size="18.0" />
      </font></Label>
   <Button fx:id="buttonIzmijeniPredmet" mnemonicParsing="false" onAction="#izmijeniPredmet" prefHeight="10.0" prefWidth="110.0" text="Izmijeni predmet" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
      <GridPane.margin>
         <Insets top="25.0" />
      </GridPane.margin></Button>
   <Button fx:id="izvjestajPbutton" mnemonicParsing="false" onAction="#izvjestajProfesora" prefHeight="10.0" prefWidth="110.0" text="Izvjestaj" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
      <GridPane.margin>
         <Insets top="25.0" />
      </GridPane.margin>
   </Button>
  
</GridPane>
