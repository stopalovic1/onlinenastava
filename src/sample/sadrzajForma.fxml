      <font>
         <Font size="25.0" />
      </font></Label>
   <Button fx:id="buttonIzmijeniOpis" mnemonicParsing="false" onAction="#izmijeniOpis" prefHeight="50.0" prefWidth="110.0" text="Izmijeni opis" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
  <rowConstraints>
    <RowConstraints maxHeight="70.0" minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
