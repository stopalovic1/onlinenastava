      <TableColumn fx:id="colSadrzajDatumCasa" prefWidth="82.0" text="Datum Casa" />
      <TableColumn fx:id="colSadrzajDatumObjave" prefWidth="97.0" text="Datum Objave" />
      <TableColumn fx:id="colSadrzajIzostaliUcenici" prefWidth="180.0" text="Izostali ucenici" />
    </columns>
  </TableView>
   <Button fx:id="buttonOpis" mnemonicParsing="false" onAction="#dodajOpis" prefHeight="50.0" prefWidth="110.0" text="Dodaj opis" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
   <Button fx:id="buttonObrisi" mnemonicParsing="false" onAction="#obrisiOpis" prefHeight="50.0" prefWidth="110.0" text="Obrisi opis" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
